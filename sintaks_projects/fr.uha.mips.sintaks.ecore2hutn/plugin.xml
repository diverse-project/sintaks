           adaptable="false"
           id="fr.uha.mips.sintaks.ecore2hutn.ui.objectContribution"
           nameFilter="*.ecore"
           objectClass="org.eclipse.core.resources.IFile">
         <menu
               id="org.kermeta.sintaks.subMenu"
               label="Sintaks"
               path="additions">
            <separator name="sintaksgroup1"/>
         </menu>
         <action
               class="fr.uha.mips.sintaks.ecore2hutn.ui.Ecore2HUTNAction"
               enablesFor="1"
               icon="icons/Sintaks.gif"
               id="fr.uha.mips.sintaks.ecore2hutn.action"
               label="Ecore to sintaks in HUTN "
               menubarPath="org.kermeta.sintaks.subMenu/sintaksgroup1"/>
